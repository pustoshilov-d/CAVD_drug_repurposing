npts 49 40 40                        # num.grid points in xyz
gridfld 67tar.maps.fld               # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C OA N SA HD        # receptor atom types
ligand_types A NA C OA N             # ligand atom types
receptor 67tar.pdbqt                 # macromolecule
gridcenter -0.000 0.000 -0.117       # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map 67tar.A.map                      # atom-specific affinity map
map 67tar.NA.map                     # atom-specific affinity map
map 67tar.C.map                      # atom-specific affinity map
map 67tar.OA.map                     # atom-specific affinity map
map 67tar.N.map                      # atom-specific affinity map
elecmap 67tar.e.map                  # electrostatic potential map
dsolvmap 67tar.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
